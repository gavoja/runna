  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Break (exit error code)",
      "program": "${workspaceFolder}\\index.js",
      "args": ["build:js", "-f", "proj5"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Build",
      "program": "${workspaceFolder}\\index.js",
      "args": ["clean - build:html build:css build:js", "-f proj1,proj2"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Dev",
      "program": "${workspaceFolder}\\index.js",
      "args": ["+serve clean - build:html build:css build:js - serve:reload", "-w", "..", "-f", "proj1,proj2"]
    }
  ]
}